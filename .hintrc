{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 114",
    "not chrome <= 114",
    "not edge <= 114",
    "not opera <= 99",
    "not samsung <= 21"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-tap-highlight-color",
          "text-decoration-skip-ink"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "apple-touch-icons": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ]
  }
}